<h1>Listing company branches</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Address</th>
    </tr>
  </thead>

  <tbody>
    <% @company_branches.each do |branch| %>
      <tr>
        <th scope="row"><%= branch.id %></th>
        <td> <%= branch.name %> </td>
        <td> <%= branch.address %></td>
      </tr>
    <% end %>
  </tbody>
</table>
